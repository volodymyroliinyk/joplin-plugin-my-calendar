{
  "name": "joplin-plugin-my-calendar",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist node_modules package-lock.json",
    "build": "webpack",
    "pack": "npm run build && cd dist && tar --format=ustar -cf ../publish/com.volodymyroliinyk.joplin.plugin.my-calendar.jpl manifest.json index.js main ui",
    "dist": "npm run pack",
    "test": "TZ=UTC jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "joplin-plugin"
  ],
  "devDependencies": {
    "@types/jest": "^29.x",
    "copy-webpack-plugin": "^12.0.0",
    "jest": "^29.x",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.x",
    "ts-loader": "^9.5.1",
    "typescript": "^5.9.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4"
  }
}
